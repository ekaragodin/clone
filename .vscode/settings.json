{
    "deno.enable": true,
    "deno.unstable": true,
    "deno.lint": true,
    "deno.import_map": "./import_map.json",
    "deno.import_intellisense_origins": {
        "https://deno.land": true,
        "https://denopkg.com": true
    },
    "deno.import_intellisense_autodiscovery": true,
    "[typescript]": {
        "editor.defaultFormatter": "denoland.vscode-deno",
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "denoland.vscode-deno",
    },
    "[javascript]": {
        "editor.defaultFormatter": "denoland.vscode-deno",
    },
}